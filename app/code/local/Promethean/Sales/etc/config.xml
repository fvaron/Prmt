<config>
    <modules>
        <Promethean_Sales>
            <version>0.1.0</version>
        </Promethean_Sales>
    </modules>
    <global>
        <models>
            <sales>
                <rewrite>
                    <quote_address>Promethean_Sales_Model_Quote_Address</quote_address>
                </rewrite>
            </sales>
        </models>
        <resources>
            <promethean_sales_setup>
                <setup>
                    <module>Promethean_Sales</module>
                    <class>Mage_Sales_Model_Resource_Setup</class>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </promethean_sales_setup>
        </resources>
    </global>
</config>